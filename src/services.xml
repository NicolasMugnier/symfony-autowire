<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="NicolasMugnier\Autowire\Domain" autowire="true" public="true"/>
        <service id="NicolasMugnier\Autowire\Client" autowire="true"/>
        <defaults autowire="true" autoconfigure="true">
            <bind key="NicolasMugnier\Autowire\ClientInterface $client" type="service" id="NicolasMugnier\Autowire\Client"/>
        </defaults>
    </services>
</container>